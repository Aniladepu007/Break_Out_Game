package my_main;

public class thread1 extends Thread{

	//constructor
	public thread1() {
		
		
		
	}
	
	public void run() {
		
		checkCollisions();
		theBall.update();

		thePaddle.update();
		
		for(PowerUp pu : powerUps) {
			pu.update();
		}
		
		if(((System.nanoTime() - screenShakeTimer) / 1000000 > 300) && ScreenShakeActive) {
			ScreenShakeActive = false;
		}
		
	}
	
}
